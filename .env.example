# GameDealer Bot Configuration
# Copy this file to .env and fill in your values

# Discord Bot Token (required)
# Get from: https://discord.com/developers/applications
DISCORD_TOKEN=your_discord_bot_token_here

# Discord Channel IDs (required)
# Right-click channel in Discord > Copy ID (requires Developer Mode)
LOG_CHANNEL_ID=1234567890123456789
DEALS_CHANNEL_ID=1234567890123456789

# ITAD API Key (required for deal fetching)
# Get from: https://isthereanydeal.com/apps/my/
ITAD_API_KEY=your_itad_api_key_here

# Debug API Responses (optional)
# Set to 'true' to log full API responses to logs/api_responses.json
DEBUG_API_RESPONSES=false